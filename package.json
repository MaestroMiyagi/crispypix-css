{
  "name": "crispypix-css",
  "version": "0.1.0",
  "description": "A pixel-perfect, 8-bit style CSS framework built with vanilla CSS",
  "main": "dist/crispypix.css",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production npm run clean && rollup -c",
    "dev": "rollup -c -w",
    "serve": "rollup -c -w"
  },
  "keywords": [
    "css",
    "framework",
    "pixel",
    "8-bit"
  ],
  "author": "MaestroMiyagi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MaestroMiyagi/crispypix-css.git"
  },
  "bugs": {
    "url": "https://github.com/MaestroMiyagi/crispypix-css/issues"
  },
  "homepage": "https://github.com/MaestroMiyagi/crispypix-css#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "postcss": "^8.4.24",
    "rimraf": "^6.0.1",
    "rollup": "^2.75.6",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-generate-html-template": "^1.7.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.1"
  }
}
